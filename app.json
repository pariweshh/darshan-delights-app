{
  "expo": {
    "name": "Darshan Delights",
    "slug": "darshan-delights-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_square.png",
    "scheme": "darshandelights",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff9300"
    },
    "ios": {
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to provide secure and convenient authentication for accessing your account.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.darshandelights.identifier",
      "associatedDomains": [
        "applinks:darshandelights.com.au",
        "applinks:www.darshandelights.com.au"
      ],
      "entitlements": {
        "aps-environment": "development",
        "com.apple.developer.in-app-payments": ["merchant.com.darshandelights"]
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ff9300"
      }
    },
    "android": {
      "package": "com.darshandelights.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_square.png",
        "backgroundColor": "#ff9300"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.android.launcher.permission.INSTALL_SHORTCUT",
        "POST_NOTIFICATIONS"
      ],

      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/shop/products"
            },
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/shop/*"
            },
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/basket"
            },
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/orders"
            },
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/contact-us"
            },
            {
              "scheme": "https",
              "host": "darshandelights.com.au",
              "pathPrefix": "/faq"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/shop/products"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/shop/*"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/basket"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/orders"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/contact-us"
            },
            {
              "scheme": "https",
              "host": "www.darshandelights.com.au",
              "pathPrefix": "/faq"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ff9300"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_square.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Darshan Delights to use Face ID for secure authentication"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo_square.png",
          "color": "#F97316",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deplymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9dcf810-5b8e-4698-992a-f11a300d8f38"
      }
    },
    "owner": "darshan-delights"
  }
}
